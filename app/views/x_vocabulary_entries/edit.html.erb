<h1>Edit DAMS Vocabulary Entry</h1>
<%= form_for @x_vocabulary_entry do |f| %>
  <%= label_tag 'x_vocabulary_entry[code]', "Code" %><%= text_field_tag 'x_vocabulary_entry[code][]', @x_vocabulary_entry.code[0] %>
  <%= label_tag 'x_vocabulary_entry[value]', "Value" %><%= text_field_tag 'x_vocabulary_entry[value][]', @x_vocabulary_entry.value[0] %>
  <%= label_tag 'x_vocabulary_entry[valueURI]', "ValueURI" %><%= text_field_tag 'x_vocabulary_entry[valueURI][]', @x_vocabulary_entry.valueURI[0] %>
  <%= label_tag 'x_vocabulary_entry[x_vocabulary]', "Vocabulary" %><%= collection_select(:x_vocabulary_entry, :x_vocabulary_id, XVocabulary.all, :id, :description, :selected => @x_vocabulary_entry.x_vocabulary.pid ) %>

  <br>
  <%=f.submit "Save", class: "btn btn-primary" %>
  <%= link_to "Cancel", x_vocabulary_entry_path( @x_vocabulary_entry ), class: "btn btn-primary" %>
<%end%>
