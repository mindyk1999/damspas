<h1>Create a new DAMS Vocabulary Entry</h1>
<%= form_for @x_vocabulary_entry do |f| %>
  <%= label_tag 'x_vocabulary_entry[code]', "Code" %><%= text_field_tag 'x_vocabulary_entry[code]' %>
  <%= label_tag 'x_vocabulary_entry[value]', "Value" %><%= text_field_tag 'x_vocabulary_entry[value]' %>
  <%= label_tag 'x_vocabulary_entry[valueURI]', "Value URI" %><%= text_field_tag 'x_vocabulary_entry[valueURI]' %>
  <%= label_tag 'x_vocabulary_entry[x_vocabulary]', "Vocabulary" %><%= collection_select(:x_vocabulary_entry, :x_vocabulary_id, XVocabulary.all, :id, :description ) %>
  <br>
  <%=f.submit "Save", class: "btn btn-primary" %>
<%end%>
