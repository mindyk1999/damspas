<% @page_title =  "Object | #{render_document_show_field_value(:document=>@document, :field=>'title_tesim')}" %>

<div class="row-fluid">
	<div class="dams-breadcrumbs span12">
		<small><a href="/">BREADCRUMBS GO HERE</a> &raquo; <%= render_document_show_field_value(:document=>@document, :field=>"title_tesim") %></small>
	</div>
</div>

<div class="row-fluid">
  <div id="content" class="show-document span9">
	
		<div class="row-fluid">
			<section class="dams-viewer span12">
				<%= render :partial => 'file_navigation' %>
			</section>
		</div>

		<div class="row-fluid">
			<section class="dams-metadata span12">
				<%= render :partial => 'metadata_alt' %>
			</section>
		</div>

		<div class="row-fluid">
			<section class="dams-comments span12">
				<%= render :partial => 'comments' %>
			</section>
		</div>
		
		<%# render 'previous_next_doc' %>
		<%# @page_title = t('blacklight.search.show.title', :document_title => document_show_html_title, :application_name => application_name) %>
		<%# extra_head_content << render_link_rel_alternates %>
		<%# sidebar_items << render_document_sidebar_partial %>

	 </div>
	 <div class="span3">

		<div class="row-fluid">
			<section class="dams-sidebar span12">
				<%if @document['component_count_isi'] != nil   %>
					<%= render :partial => 'metadata_components' %>
				<% end %>
				<%= render :partial => 'related_resources' %>
				<%# render :partial => 'catalog/show_tools' %>
			</section>
		</div>

	</div>
</div>

